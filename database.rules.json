{
  "rules": {
    "users": {
      "$uid": {
        ".read": "auth != null && $uid === auth.uid",
        ".write": "auth != null && $uid === auth.uid"
      }
    },
    "key": {
      ".read": false,
      ".write": false
    },
    "resources": {
      ".read": true,
      ".write": false
    }
  }
}